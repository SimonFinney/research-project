<!DOCTYPE html>
<html class="html no-js" lang="en" {%- if isDebug %} data-debug {%- endif -%}>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimal-ui">
    <title>{{ name }}</title>
    <link rel="stylesheet" href="/css/main.css">
  </head>
  <body class="body" data-app data-dialog data-variation="{{ variation }}">
    <main class="main">

      {%- block content -%}
      {%- endblock -%}
    </main>

    {%- include 'includes/partials/_dialog.nunjucks' %}

    {% block script -%}
    {%- endblock -%}
  </body>
